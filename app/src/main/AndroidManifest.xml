<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.diplomskirad.celestin.diplomskiv2">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Postavke"
            android:label="@string/title_activity_postavke"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".SentronActivity"
            android:label="@string/title_activity_sentron"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".AcsActivity"
            android:label="@string/title_activity_acs"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>